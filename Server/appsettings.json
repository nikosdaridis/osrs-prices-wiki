{
  "SerilogValues": {
    "Paths": {
      "Linux": "/var/log/serilog/.log",
      "Windows": "C:\\logs\\osrspriceswiki\\.log"
    },
    "MinimumLevel": "Information",
    "FileSizeLimitBytes": 209715200,
    "RollingInterval": "Day",
    "RollOnFileSizeLimit": true,
    "Shared": true,
    "FlushToDiskIntervalSeconds": 1,
    "RetainedFileCountLimit": null,
    "FormatProviderCulture": "en-US"
  },
  "SMTPValues": {
    "Server": "smtp.gmail.com",
    "Port": 587,
    "User": "email@gmail.com",
    "Pass": "password",
    "To": "email@gmail.com"
  },
  "OsrsWikiValues": {
    "OSRSPricesWikiBaseUri": "https://prices.runescape.wiki/api/v1/osrs",
    "OldschoolWikiBaseUri": "https://oldschool.runescape.wiki",
    "OldschoolWikiIconsBaseUri": "https://oldschool.runescape.wiki/images",
    "Latest": "/latest",
    "Mapping": "/mapping",
    "Timeseries": "/timeseries?timestep={timestep}&id={id}",
    "Volume": "/?title=Module:GEVolumes/data.json&action=raw&ctype=application%2Fjson"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
